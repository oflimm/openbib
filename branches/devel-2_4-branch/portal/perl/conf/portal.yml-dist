# Web-Administration
  - # admin_database_loc: Collection Overview
     module: 'OpenBib::Handler::Apache::Admin::DatabaseRSS'
     rule: '/portal/admin/database/:databaseid/rss.html[get]'
     runmode: 'show_collection_as_html'
