# Suchprefixe in der Rercherche fuer die internen Felder person, title usw.
# Zuordnung: Interner Bezeichnung -> Suchprefix
searchfield:
  'id':
     'prefix': 'id'
     'type': 'id'
  'personid':
     'prefix': 'perid'
     'type': 'id'
  'corporatebodyid':
     'prefix': 'corpid'
     'type': 'id'
  'classificationid':
     'prefix': 'clnid'
     'type': 'id'
  'subjectid':
     'prefix': 'subjid'
     'type': 'id'
  'freesearch':
     'prefix': 'fs'
     'type': 'ft'
     'option':
        'filter_isbn': 1
        'filter_issn': 1
  'person':
     'prefix': 'per'
     'type': 'ft'
  'title':
     'prefix': 'tit'
     'type': 'ft'
  'subject': 
     'prefix': 'subj'
     'type': 'ft'
  'corporatebody':
     'prefix': 'corp'
     'type': 'ft'
  'classification':
     'prefix': 'cln'
     'type': 'ft'
  'mark': # Signatur
     'prefix': 'mark'
     'type': 'ft'
  'isbn':
     'prefix': 'isbn'
     'type': 'ft'
     'option':
        'filter_isbn': 1
  'issn':
     'prefix': 'issn'
     'type': 'ft'
     'option':
        'filter_issn': 1
  'mediatype':
     'prefix': 'typ'
     'type': 'ft'
  'content': 
     'prefix': 'cnt'
     'type': 'ft'
  'source': # gtquelle
     'prefix': 'src'
     'type': 'ft'
  'year': # fest inkl. prefix
     'prefix': 'year'
     'type': 'ft'
  'subid':
     'prefix': 'fsub'
     'type': 'id'
  'titlestring':
     'prefix': 'ftit'
     'type': 'string'
     'option':
       'strip_first_stopword': 1
  'subjectstring':
     'prefix': 'fsubj'
     'type': 'string'
  'languagestring':
     'prefix': 'flang'
     'type': 'string'
  'classificationstring':
     'prefix': 'fsys'
     'type': 'string'
  'personstring':
     'prefix': 'fper'
     'type': 'string'
  'corporatebodystring':
     'prefix': 'fcorp'
     'type': 'string'
  'mediatypestring':
     'prefix': 'ftyp'
     'type': 'string'
  'markstring':
     'prefix': 'fmark'
     'type': 'string'
  'yearstring':
     'prefix': 'fyear'
     'type': 'string'
  'bibkey':
     'prefix': 'bkey'
     'type': 'string'

  # Suche nach speziellen Kategorien
  't0531':
     'prefix': 't0531'
     'type': 'string'
  't0533':
     'prefix': 't0533'
     'type': 'string'
  't4100':
     'prefix': 't4100'
     'type': 'string'
  't4300':
     'prefix': 't4300'
     'type': 'string'

# xapian index basepath
xapian_index_base_path: /opt/openbib/ft/xapian/index
xapian_option:
  maxmatch: 2000
  max_key_length: 240

# Suchprefixe in Xapian fuer die Suchprefixe in der Rercherche
# entsprechend searchfield_prefix
# Zuordnung: Suchprefix ->  Xapian-Term/Posting-Prefix
xapian_search_prefix:
  'id': 'Q'
  'bkey': 'B'

  'perid': 'I1'
  'corpid': 'I2'
  'subjid': 'I3'
  'clnid': 'I4'

  'fs': ''

# Volltext
  'per': 'X1'
  'tit': 'X2'
  'corp': 'X3'
  'subj': 'X4'
  'sys': 'X5'
  'mark': 'X6'
  'year': 'X7'
  'isbn': 'X8'
  'issn': 'X9'
  'typ': 'Y1'
  'cnt': 'Y2'
  'src': 'Y3'

  # String / Drilldowns
  'fsubj': 'D1'
  'fcln': 'D2'
  'fper': 'D3'
  'ftyp' : 'D4'
  'fyear': 'D5'
  'flang': 'D6'
  'fcorp': 'D7'
  'fdb': 'D8'
  'ftit': 'D9'
  'fmark': 'E1'
  'fsub': 'E2'

  # Einzelne Kategorieinhalte
  't0531': 'T0531'
  't0533': 'T0533'
  't4100': 'T4100'
  't4300': 'T4300'

xapian_drilldown_value:
  person: 3
  mediatype: 4
  classification: 2
  language: 6
  subject: 1
  year: 5
  corporatebody: 7
  db: 8

xapian_sorttype_value:
  'person': 20
  'title': 21
  'order': 22
  'year': 23
  'publisher': 24
  'mark': 25
  'popularity': 26

