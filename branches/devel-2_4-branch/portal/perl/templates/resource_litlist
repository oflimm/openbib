[%- PROCESS common/subtemplate name="local_config" -%] 
[%- IF representation == '' || representation == 'html' -%]
[%- PROCESS common/subtemplate name="resource_litlist_html" -%]
[%- ELSIF representation == 'rdf' -%]
[%- PROCESS common/subtemplate name="resource_litlist_rdf" -%]
[%- ELSIF representation == 'json' -%]
[%- PROCESS common/subtemplate name="resource_litlist_json" -%]
[%- ELSIF representation == 'rss' -%]
[%- PROCESS common/subtemplate name="resource_litlist_rss" -%]
[%- ELSIF representation == 'include' -%]
[%- PROCESS common/subtemplate name="resource_litlist_include" -%]
[%- ELSIF representation == 'bibtex' -%]
[%- PROCESS common/subtemplate name="resource_litlist_bibtex" -%]
[%- ELSIF representation == 'refer' -%]
[%- PROCESS common/subtemplate name="resource_litlist_refer" -%]
[%- ELSE -%]
This Resource-Type is not available in the representation [% representation %]
[%- END -%]
