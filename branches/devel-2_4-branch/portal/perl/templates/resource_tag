[%- IF representation == '' || representation == 'html' -%]
[%- PROCESS common/subtemplate name="resource_tag_html" -%]
[%- ELSIF representation == 'rdf' -%]
[%- PROCESS common/subtemplate name="resource_tag_rdf" -%]
[%- ELSIF representation == 'json' -%]
[%- PROCESS common/subtemplate name="resource_tag_json" -%]
[%- ELSIF representation == 'bibtex' -%]
[%- PROCESS common/subtemplate name="resource_tag_bibtex" -%]
[%- ELSIF representation == 'refer' -%]
[%- PROCESS common/subtemplate name="resource_tag_refer" -%]
[%- ELSE -%]
This Resource-Type is not available in the representation [% representation %]
[%- END -%]
