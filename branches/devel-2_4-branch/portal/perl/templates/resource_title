[%- IF representation == '' || representation == 'html' -%]
[%- PROCESS common/subtemplate name="resource_title_html" -%]
[%- ELSIF representation == 'rdf+xml' -%]
[%- PROCESS common/subtemplate name="resource_title_rdf+xml" -%]
[%- ELSIF representation == 'json' -%]
[%- PROCESS common/subtemplate name="resource_title_json" -%]
[%- ELSIF representation == 'bibtex' -%]
[%- PROCESS common/subtemplate name="resource_title_bibtex" -%]
[%- ELSIF representation == 'refer' -%]
[%- PROCESS common/subtemplate name="resource_title_refer" -%]
[%- ELSE -%]
This Resource-Type is not available in the representation [% representation %]
[%- END -%]
