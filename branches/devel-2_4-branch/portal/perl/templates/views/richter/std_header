<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
  <head>
[% IF NOT disable_cache_control %]
    <meta http-equiv="content-type"  content="text/html; charset=utf-8"/>
    <meta http-equiv="Pragma"        content="no-cache" />
    <meta http-equiv="Cache-Control" content="private,no-cache,no-store" />
    <meta http-equiv="Expires"       content="Thu, 01 Dec 1970 12:00:00 GMT" />
[% END %]
    
[% IF record %]
    <meta name="Bibkey"              content="[% record.to_bibkey %]" />
    <meta name="Tags"                content="[% record.to_tags %]" />
[% END %]
[% IF NOT config.strip_view_from_uri(view) %]
    <meta name="View"                content="[% view %]" />
[% END %]
<link href="/images/openbib/views/richter/favicon.ico" rel="shortcut icon" />
[% IF view && primrssfeed %]
<link rel="alternate" type="application/rss+xml" title="RDF-Datei" href="[% primrssfeed %]" />
[% END %]
[% IF LinkedData == "title" %]
<link rel="alternate" type="application/rdf+xml" title="RDF Representation" href="http://[% servername %][% path_prefix %]/[% config.get('title_loc') %]/[%- record.database -%]/[%- record.id -%].rdf"/>
<link rel="alternate" type="application/json" title="JSON Representation" href="http://[% servername %][% path_prefix %]/[% config.get('title_loc') %]/[%- record.database -%]/[%- record.id -%].json"/>
[% END %]
[% IF unapi && config.get('unAPI') %]
    <link rel="unapi-server" title="unAPI" type="application/xml" href="[% path_prefix %]/[% config.get('connector_unapi_loc') %]" />
[% END %]

  [%- TRY -%]
    [%- USE File "${config.get('document_root_path')}${config.get('css_loc')}/profile/$sysprofile/views/$view/layout.css" -%]
    <link rel="stylesheet" type="text/css" href="[% "${config.get('css_loc')}/profile/$sysprofile/views/$view/layout.css" %]" media="all" />
  [%- CATCH -%]        
    [%- TRY -%]
      [%- USE File "${config.get('document_root_path')}${config.get('css_loc')}/profile/$sysprofile/layout.css" -%]
    <link rel="stylesheet" type="text/css" href="[% "${config.get('css_loc')}/profile/$sysprofile/layout.css" %]" media="all" />
    [%- CATCH -%]
      [%- TRY -%]
        [%- USE File "${config.get('document_root_path')}${config.get('css_loc')}/views/$view/layout.css" -%]
    <link rel="stylesheet" type="text/css" href="[% "${config.get('css_loc')}/views/$view/layout.css" %]" media="all" />
      [%- CATCH -%]
    <link rel="stylesheet" type="text/css" href="[% "${config.get('css_loc')}/layout.css" %]" media="all" />
      [%- END -%]
    [%- END -%]
  [%- END -%]

