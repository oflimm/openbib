[%- ELSIF representation == 'rdf' -%]
[%- PROCESS common/subtemplate name="litlists_item_record_rdf" -%]
[%- ELSIF representation == 'json' -%]
[%- PROCESS common/subtemplate name="litlists_item_record_json" -%]
[%- ELSIF representation == 'rss' -%]
[%- PROCESS common/subtemplate name="litlists_item_record_rss" -%]
[%- ELSIF representation == 'include' -%]
[%- PROCESS common/subtemplate name="litlists_item_record_include" -%]
[%- ELSIF representation == 'bibtex' -%]
[%- PROCESS common/subtemplate name="litlists_item_record_bibtex" -%]
[%- ELSIF representation == 'refer' -%]
[%- PROCESS common/subtemplate name="litlists_item_record_refer" -%]
[%- ELSE -%]
This Resource-Type is not available in the representation [% representation %]
