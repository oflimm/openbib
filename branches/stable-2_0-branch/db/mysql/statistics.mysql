-------------------------------------------------
--------------- T A B E L L E N -----------------
-------------------------------------------------

drop table IF EXISTS relevance;
CREATE TABLE relevance (
 id          VARCHAR(40),
 isbn        VARCHAR(15),
 dbname      VARCHAR(25),
 katkey      INT,
 INDEX (id),
 INDEX (isbn),
 INDEX (dbname),
 INDEX (katkey)
)
DEFAULT CHARACTER SET utf8
COLLATE utf8_general_ci
PACK_KEYS=1;

