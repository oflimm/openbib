# Parametrisierung der Sortierung
    sorting_order:
      - 'P0800'
      - 'C0800'
      - 'S0800'
      - 'PC0001:a'
      - 'T0245:a'
      - 'T5100:a'
      - 'T0264:c' 
      - 'T0246:b'
      - 'X0014:a'
      - 'popularity'
    sorting:
      'P0800':
        'sortfield': 'authority' # Normdaten-Ansetzungsform Person
        'type': 'stringfield'
      'C0800':
        'sortfield': 'authority' # Normdaten-Ansetzungsform Koerperschaft
        'type': 'stringfield'
      'S0800':
        'sortfield': 'authority' # Normdaten-Ansetzungsform Schlagwort
        'type': 'stringfield'
      'PC0001:a':
        'sortfield': 'person'    # Verfasser/Koepeschaft
        'type': 'stringfield'
      'T0245:a':                   # Titel
        'sortfield': 'title'
        'type': 'stringfield'
        'filter':
           - 'filter_strip_nonsortable_word'
      'T5100:a':
        'sortfield': 'order'     # Zaehlung
        'type': 'integerfield'
      'T0264:c':
        'sortfield': 'year'      # Jahr
        'type': 'integerfield'       
        'filter':
           - 'filter_force_signed_year'
      'T0246:b':
        'sortfield': 'publisher' # Verlag
        'type': 'stringfield'
      'X0014:a':
        'sortfield': 'mark'      # Signatur
        'type': 'stringfield'
      'popularity':
        'sortfield': 'popularity'# Popularitaet
        'type': 'integervalue'

