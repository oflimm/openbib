# Einstellungen fuer SOLR Suchindex

solr:
# Backend-Specific  
  local: # Lokale Indexierung mit Solr
    base_url: ''
    search_url: ''
  gvi:   # Zugriff auf Solr Index des GVI
    base_url: ''
    search_url: ''

    
solr_filter_field:
  local:
    fsubj: facet_subject
    fcls: facet_classification
    fper: facet_person
    fyear: facet_year
    ftyp: facet_mediatype
    flang: facet_language
    fcorp: facet_corporatebody
    fdb: facet_database
    ftag: facet_tag
    flitlist: facet_litlist
    floc: facet_location
    favail: facet_availability
    topic: facet_topic
    fprov: facet_provenance
  gvi:
    fsubj: subject_all_facet
    fper: author_facet
    fyear: publish_date
    ftyp: material_pubform

# https://github.com/gemeinsamerverbuendeindex/gvi/blob/develop/solr_config/schema.xml

solr_searchfield_mapping:    
  gvi: 
    'freesearch': 'allfields'
    'person': 'author'
    'corporatebody': 'author'	    
    'title': 'title'
    'titlestring': 'title_phrase'
    'publisher': 'publisher'
    'zdbid': 'zdb_id'
    'isbn': 'isbn'
    'issn': 'issn'
    'year': 'publish_date'
    'content': 'summary'
    'subject': 'subject_all'

solr_facet_mapping:    
  gvi: 
    'person': 'author_facet'
    'subject': 'subject_all_facet'
    'year': 'publish_date'
    'mediatype': 'material_pubform'


# Einstellungen fuer EDS Suchindex

eds:
  default_user: ''
  default_password: ''
  default_profile: 'newapi'
  
  base_url: 'https://adn-eds-api.ebscohost.com'
  auth_url: 'https://adn-eds-api.ebscohost.com/authservice/rest/uidauth'
  info_url: 'https://adn-eds-api.ebscohost.com/authservice/rest/info'
  session_url: 'https://adn-eds-api.ebscohost.com/edsapi/rest/createsession'
  search_url: 'https://adn-eds-api.ebscohost.com/edsapi/rest/search'
  retrieve_url: 'https://adn-eds-api.ebscohost.com/edsapi/rest/retrieve'  

eds_searchfield_mapping:
  freesearch: 'TX'
  person: 'AU'  
  personstring: 'AU'  
  title: 'TI'
  titlestring: 'TI'  
  subject: 'SU'
  subjectstring: 'SU'
  content: 'AB' # abstract
  issn: 'IS'
  isbn: 'IB'
  year: 'DT'
  sourcestring: 'SO' # source

eds_facet_mapping:
  PublicationYear: 'year'
  SourceType: 'mediatype'  
  SubjectEDS: 'subject'
  Journal: 'journal'
  Publisher: 'publisher'
  Language: 'language'
  ContentProvider: 'source'

eds_reverse_facet_mapping:
  'fyear': PublicationYear
  'ftyp': SourceType
  'fsubj': SubjectEDS
  'fjournal': Journal
  'fpub': Publisher
  'flang': Language
  'fsrc': ContentProvider

eds_reverse_sort_mapping:
  year_desc: 'date'
  year_asc: 'date2'
  relevance_desc: 'relevance'
  person_asc: 'author'

