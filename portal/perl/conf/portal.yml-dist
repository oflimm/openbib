# Anzahl der Spalten bei der Datenbankauswahlseite
databasechoice_maxcolumn: 1

# Verschiedene Pfadangaben
autoconv_dir: /opt/openbib/autoconv
base_dir: /opt/openbib
conv_dir: /opt/openbib/conv
dbdesc_dir: /opt/openbib/db
tool_dir: /opt/openbib/bin

# Zugriffsinformationen fuer das BibSonomy-API
bibsonomy_api_key: ''
bibsonomy_api_user: ''

# Journaltocs
journaltocs_api_user: ''

# Zugriffsinformationen fuer die LibraryThing-APIs
librarything_api_key: ''

# Zugriffsinformationen fuer ReCaptcha
recaptcha_user: ''
recaptcha_public_key: ''
recaptcha_private_key: ''

# Zugriffsinformationen fuer recensio.net

rezensionen_online_key: ''

# Kurztiteltabelle verwenden?
use_titlistitem_table: 1

# Internes Serialisierungsformat, z.B. zur Ablage-Format in der Tabelle (json oder packed_storable)
internal_serialize_type: 'json'

# Maximale Laenge an Inhalt in Kurztitelliste, ab der Personen
# gekuerzt werden soll
maxlength_person: 100
maxlength_title: 100
maxlength_mark: 70
maxlength_dbname: 70

# Maximale Anzahl erlaubter Login-Fehlversuche bevor das Account gesperrt wird
max_login_failure: 10

# https fuer Admin und Nutzer verwenden
use_https: 1
use_admin_https: 1

# Testsystem
testsystem: 0

# Facets
facets:
  subject: 1
  classification: 2
  person: 3
  year: 5
  mediatype: 4
  language: 6
  corporatebody: 7
  database: 8
  tag: 9
  litlist: 10
  location: 11
  availability: 12
  topic: 13
  provenance: 14
  custom1: 60
  custom2: 61
  custom3: 62
  custom4: 63
  custom5: 64 
  custom6: 65
  custom7: 66
  custom8: 67
  custom9: 68
  custom10: 69

# Repraesentationen

representation:
   'xml': 'application/xml'
   'rdf+xml': 'application/rdf+xml'
   'rdf+n3': 'application/rdf+n3'
   'json': 'text/plain'
   'bibtex': 'text/plain'
   'refer': 'text/plain'
   'mobile': 'text/html'
   'html': 'text/html'
   'ics': 'text/calendar'
   'csv': 'text/csv'
   'txt': 'text/plain'
   'mods': 'application/mods+xml'
   'rss': 'application/rss+xml'

