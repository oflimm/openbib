# Anzahl der Spalten bei der Datenbankauswahlseite
databasechoice_maxcolumn: 1

# Verschiedene Pfadangaben
autoconv_dir: /opt/openbib/autoconv
base_dir: /opt/openbib
conv_dir: /opt/openbib/conv
dbdesc_dir: /opt/openbib/db
tool_dir: /opt/openbib/bin

# Zugriffsinformationen fuer das BibSonomy-API
bibsonomy_api_key: ''
bibsonomy_api_user: ''

# Journaltocs
journaltocs_api_user: 'portal@foo.bar'

# Zugriffsinformationen fuer die LibraryThing-APIs
librarything_api_key: ''

# Zugriffsinformationen fuer ReCaptcha
recaptcha_user: ''
recaptcha_public_key: ''
recaptcha_private_key: ''

# Zugriffsinformationen fuer recensio.net

rezensionen_online_key: ''

# Aktivierung verschiedener Funktionalitaeten
login_active: 1
newsflash: 1

# Sollen Performance-Benchmarks durchgefuehrt werden
benchmark: 0

# Kurztiteltabelle verwenden?
use_titlistitem_table: 1

# Internes Serialisierungsformat, z.B. zur Ablage-Format in der Tabelle (json oder packed_storable)
internal_serialize_type: 'json'

# Maximale Laenge an Inhalt in Kurztitelliste, ab der Personen
# gekuerzt werden soll
maxlength_person: 100
maxlength_title: 100
maxlength_mark: 70
maxlength_dbname: 70

# https fuer Admin und Nutzer verwenden
use_https: 1
use_admin_https: 1

# Testsystem
testsystem: 0

# Facets
facets:
  subject: 1
  classification: 2
  person: 3
  year: 5
  mediatype: 4
  language: 6
  corporatebody: 7
  database: 8
  tag: 9
  litlist: 10
  location: 11
  availability: 12
  topic: 13
  provenance: 14

# Repraesentationen

representation:
   'xml': 'application/xml'
   'rdf+xml': 'application/rdf+xml'
   'rdf+n3': 'application/rdf+n3'
   'json': 'text/plain'
   'bibtex': 'text/plain'
   'refer': 'text/plain'
   'mobile': 'text/html'
   'html': 'text/html'
   'ics': 'text/calendar'
   'csv': 'text/csv'
   'txt': 'text/plain'

# Default Query-Options (Session)
queryoptions:
  clp: # Collapse via sorttype_value (title,authority,...)
    value: ''
    storage: none
  facets: # Generate these facets instead of defaults
    value: ''
    storage: none
  num:
    value: 20
    storage: session
  page:
    value: 1
    storage: none
  srt:
    value: 'relevance_desc'
    storage: none
  srta:
    value: 0
    storage: none
  profil:
    value: ''
    storage: none
  novrp:
    value: 0
    storage: none
  dop:
    value: 'AND'
    storage: none
  lt:
    value: 'cover'
    storage: session
  ips_user:
    value: ''
    storage: none
  sm:
    value: ''
    storage: none
  sb:
    value: 'xapian'
    storage: none
  access_green:
    value: 0
    storage: none
  access_yellow:
    value: 0
    storage: none
  access_red:
    value: 0
    storage: none
  access_national:
    value: 0
    storage: none
  access_mask:
    value: 0
    storage: none

# Generierte BestOf-Daten
best_of:
  3:
    cloud: log
  4:
    cloud: log
  5:
    cloud: log
  6:
    cloud: log
  7:
    cloud: log
