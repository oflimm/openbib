# Versionsnummer
version: OpenBib 3.2

# Unter dieser IP wird der lokale Server angesprochen
local_ip: 127.0.0.1

postgresql_version: '111200' # 11.12.0

# Einstellungen fuer den Zugriff auf die Katalog-Datenbanken
dbimodule: Pg
pgdbhost: localhost
pgdbport: 5432
pgdbname: postgres
pgdbuser: root
pgdbpasswd: TopSecret

# Einstellungen fuer den Zugriff auf die Katalog-Datenbanken
dbimodule: Pg
dbhost: localhost
dbport: 5432
dbuser: root
dbpasswd: TopSecret
dboptions:
  pg_enable_utf8: 1
#  pg_server_prepare: 0

# Einstellungen fuer den Zugriff auf die System-Datenbank
systemdbimodule: Pg
systemdbhost: localhost
systemdbport: 5432
systemdbname: openbib_system
systemdbuser: root
systemdbpasswd: TopSecret
systemdboptions:
  pg_enable_utf8: 1
#  pg_server_prepare: 0
systemdbsingleton: 0

# Einstellungen fuer den Zugriff auf die Enrichmnt-Datenbank
enrichmntdbimodule: Pg
enrichmntdbhost: localhost
enrichmntdbport: 5432
enrichmntdbname: openbib_enrichmnt
enrichmntdbuser: root
enrichmntdbpasswd: TopSecret
enrichmntdboptions:
  pg_enable_utf8: 1
#  pg_server_prepare: 0
enrichmntdbsingleton: 0


# Einstellungen fuer den Zugriff auf die Statistik-Datenbank
statisticsdbimodule: Pg
statisticsdbhost: localhost
statisticsdbport: 5432
statisticsdbname: openbib_statistics
statisticsdbuser: root
statisticsdbpasswd: TopSecret
statisticsdboptions:
  pg_enable_utf8: 1
#  pg_server_prepare: 0
statisticsdbsingleton: 0

# Aktivierung und selektives Debugging verschiedener Funktionalitaeten
debug_ils: 0
active_ils: 0
active_campusorder: 0
active_pdaorder: 0
active_login: 1 # Legacy: Ersetzt durch Definition von Login-Zielen in View-Config
active_statisticsdb: 1
newsflash: 1

# Preislimit fuer Neuanschaffungen durch PDA 
pda_limit_cent: 100

# Sollen Performance-Benchmarks durchgefuehrt werden
benchmark: 0

# Einstellungen fuer Memcached

memcached:
  servers:
     - address: '127.0.0.1:11211'
  compress_threshold: 10000
  namespace: 'ob:'
  ketama_points: 150

memcached_expiration:
  'record:title:full': 3600
  'record:title:circulation': 240
  'record:title:enrich_related': 3600
  'record:title:enrich_similar': 3600
  'record:title:enrich_same': 3600
  'config:databaseinfotable': 43200
  'config:circulationinfotable': 43200
  'config:locationinfotable': 43200
  'config:dbinfo_overview': 43200
  'config:locationinfo_overview': 43200
  'config:viewinfo_overview': 43200
  'config:locationinfo_fields': 43200
  'config:locationinfo_occupancy': 240
  'config:topics': 3600
  'config:bk': 43200
  'config:rvk': 43200
  'config:ddc': 43200
  'config:lcc': 43200
  'config:viewdbs': 43200
  'config:viewlocations': 43200
  'config:searchprofile_dbs': 43200
  'handler:connector_locationmark': 43200
  'eds:authtoken': 3600
  'eds:sessiontoken': 3600  
  'eds:title': 3600
  'eds:search': 3600      
  'dbis:classifications': 3600
  'dbis:title': 3600
  'dbis:search': 3600
  'ezb:classifications': 3600
  'ezb:title': 3600
  'ezb:search': 3600

# Parameter fuer USBWS ILS-Anbindung

usbws_url: ''
usbwsmail_url: ''
usbauth_url: ''
usbauth_masterpass: ''

# Local networks/IP ranges to recognize access to licensed media
local_networks:
  - '172.16.0.0/12' # Private Net

youtube_api_key: ''

# Authentifizierungs-Targets

authenticator:
  ldap:
    ad1:
      hostname: foo.bar
      userdn: 'USER_NAME@ad.foo.bar'
      port: 636
      scheme: 'ldaps'
      version: 3
      basedn: 'OU=FOO,dc=ad,dc=foo,dc=bar,dc=local'
      restrict_groupdn:
        "CN=Benutzer,OU=Gruppen,OU=FOO,DC=ad,DC=foo,DC=bar,DC=local": 1
    ad2:
      hostname: 'baz.bar'
      userdn: 'uid=USER_NAME,ou=People,dc=baz,dc=bar'
      port: '636'
      verify: 'require'
      timeout: '1500'
      onerror: 'undef'
      cafile: '/opt/openbib/conf/Certs.pem'
  olws:
    catalogname:
      hostname: foo.bar
      port: 3003
      remoteuser: baz
      dbname: 'catalogname'
  paia:
    inst001:
      base_url: ''

olws:
  default:
    circwsurl: ''
    circdb: ''
  catalogname:
    circwsurl: ''
    circdb: 'catalogname'

mail:
  realm:
    myrealm:
      recipient: 'myrealm@foo.bar'
      sender: 'myrealm@baz.bar'

kmb_bankverbindung_html: ""

local_search_backends:
  - name: 'xapian'
    desc: 'Xapian'
  - name: 'elasticsearch'
    desc: 'Elasticsearch'
#  - name: 'solr'
#    desc: 'SOLR'
  
default_local_search_backend: xapian

# Benutzer, Passwort und Default view fuer die Administration
adminuser: admin
adminpasswd: TopSecret

defaultview: openbib

# RSS-Typen
rss_types:
  neuerwerbung: 99
  neuzugang: 1
  neuzugang_nach_aut: 2
  neuzugang_nach_kor: 3
  neuzugang_nach_swt: 4
  neuzugang_nach_sys: 5

location_types:
  - ISIL
  - Generic

metadata_schema:
  - mab2
  - marc21

source_systems:
  Sisis:
     has_authorities: 1
  Aleph:
     has_authorities: 0
  Alma:
     has_authorities: 0
  Allegro:
     has_authorities: 0
  Aleph:
