# Einstellungen fuer Memcached

# memcached:
#   servers:
#      - address: '127.0.0.1:22122'
#   compress_threshold: 10000
#   namespace: 'ob:'
#   ketama_points: 150

memcached_expiration:
  'record:title:full': 3600
  'record:title:circulation': 240
  'record:title:enrich_related': 3600
  'record:title:enrich_similar': 3600
  'record:title:enrich_same': 3600
  'config:databaseinfotable': 43200
  'config:circulationinfotable': 43200
  'config:locationinfotable': 43200
  'config:dbinfo_overview': 43200
  'config:locationinfo_overview': 43200
  'config:viewinfo_overview': 43200
  'config:locationinfo_fields': 43200
  'config:viewdbs': 43200
  'config:viewlocations': 43200
  'config:searchprofile_dbs': 43200
  'handler:connector_locationmark': 43200

usbws_url: ''

youtube_api_key: ''

# Authentifizierungs-Targets

authenticator:
  ldap:
    ad1:
      hostname: foo.bar
      userdn: 'USER_NAME@ad.foo.bar'
      port: 636
      scheme: 'ldaps'
      version: 3
      basedn: 'OU=FOO,dc=ad,dc=foo,dc=bar,dc=local'
      restrict_groupdn:
        "CN=Benutzer,OU=Gruppen,OU=FOO,DC=ad,DC=foo,DC=bar,DC=local": 1
    ad2:
      hostname: 'baz.bar'
      userdn: 'uid=USER_NAME,ou=People,dc=baz,dc=bar'
      port: '636'
      verify: 'require'
      timeout: '1500'
      onerror: 'undef'
      cafile: '/opt/openbib/conf/Certs.pem'
  olws:
    catalogname:
      hostname: foo.bar
      port: 3003
      remoteuser: baz
      dbname: catalogname

# Einstellungen fuer ElasticSearch

elasticsearch:
  nodes:
    - '127.0.0.1:9200'
  cxn_pool: 'Sniff'

eds:
  userid: ''
  passwd: ''
  profile: 'newapi'
  
  base_url: 'https://adn-eds-api.ebscohost.com'
  auth_url: 'https://adn-eds-api.ebscohost.com/authservice/rest/uidauth'
  info_url: 'https://adn-eds-api.ebscohost.com/authservice/rest/info'
  session_url: 'https://adn-eds-api.ebscohost.com/edsapi/rest/createsession'
  search_url: 'https://adn-eds-api.ebscohost.com/edsapi/rest/search'
  retrieve_url: 'https://adn-eds-api.ebscohost.com/edsapi/rest/retrieve'  

eds_searchfield_mapping:
  freesearch: 'TX'
  person: 'AU'  
  personstring: 'AU'  
  title: 'TI'
  titlestring: 'TI'  
  subject: 'SU'
  subjectstring: 'SU'
  content: 'AB' # abstract
  issn: 'IS'
  isbn: 'IB'
  year: 'DT'
  sourcestring: 'SO' # source

eds_facet_mapping:
  PublicationYear: 'year'
  SourceType: 'mediatype'  
  SubjectEDS: 'subject'
  Journal: 'journal'
  Publisher: 'publisher'
  Language: 'language'
  ContentProvider: 'source'

eds_reverse_facet_mapping:
  'fyear': PublicationYear
  'ftyp': SourceType
  'fsubj': SubjectEDS
  'fjournal': Journal
  'fpub': Publisher
  'flang': Language
  'fsrc': ContentProvider

local_search_backend: xapian

