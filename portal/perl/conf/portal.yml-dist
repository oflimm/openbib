# Charakteristik
# - ausschliesslich oeffentliche Repraesentation [get]
# - ein kanonischer Resource-URI [get]
# - Verlinkung auf nutzerspezifische Repreaesentation anderer
#   Resourcen ueber deren Resource-URL, der entsprechend weiterleitet

#####################################################################
# Recherche-Resource/Repraesentation

  - # Resource: search_loc
     module: 'OpenBib::Handler::PSGI::Search'
     rule: '/portal/:view/search'
     representations:
       - none
       - html
       - json
       - rdf
       - rss
       - mobile
     runmode: 'show_search'
  - # Resource: search_loc
     module: 'OpenBib::Handler::PSGI::Authority::Search'
     rule: '/portal/:view/authority/search'
     representations:
       - none
       - html
       - json
       - txt
       - rss
       - mobile
     runmode: 'show_search'
  - # Resource: search_loc
     module: 'OpenBib::Handler::PSGI::Search::History'
     rule: '/portal/:view/search/history[get]'
     representations:
       - none
       - html
     runmode: 'show'

