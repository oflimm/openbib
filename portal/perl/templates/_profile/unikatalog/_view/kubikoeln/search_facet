<!-- Begin classification facet -->
[%         IF config.get('facets').classification && facets.defined('classification') && facets.classification.size > 0 %]
[%             all_rvk = [];
               FOREACH termitem IN facets.classification;
                  searchterm = termitem.0;
                  LAST IF loop.count > 25 ;
                  IF searchterm.match('^[A-Z][A-Z] \d\d\d+$');
                    all_rvk.push(searchterm);
                  END;
               END;
               rvklookup = config.get_description_of_classification(type => 'rvk', names => all_rvk );
%]
        <h4 class="ob-facet-title" data-bs-toggle="collapse" data-bs-target="#group-classification[% idsuffix %][% facet_prefix %]" aria-expanded="false">
           [% msg.maketext("Systematik") %] 
        </h4>
        <ul id="group-classification[% idsuffix %][% facet_prefix %]" class="list-group in collapse">
[%             FOREACH termitem IN facets.classification ;
                  LAST IF loop.count > 25 ;
 %]

[%-
    searchterm    = termitem.0;
    searchprefix  = config.get('searchfield').classificationstring.prefix;
    filterstring  = "${searchprefix}:${searchterm}";

    # Beschreibungen hinzufuegen
    clndesc = "";
    IF searchterm.match('^\d\d\.\d\d$') && bklookup.${searchterm};
      clndesc = bklookup.${searchterm};
    ELSIF searchterm.match('^[A-Z][A-Z] \d\d\d+$') && rvklookup.${searchterm};
      clndesc = rvklookup.${searchterm};
    END;

-%]
<li class="ob-facet-item" [% IF facets.classification.size > 5 && loop.count > 5 %]hidden[% END %]>[% IF NOT separate_refine %]<input type="checkbox" name="f[[% searchprefix %]]" value="[% uri_escape(searchterm) %]" [% IF have_filter.${filterstring} %]checked="checked"[% END %] onClick="this.form.submit()"/>&nbsp;[% END %]<a class="w-100" [% IF offcanvas %]data-bs-toggle="offcanvas"[% END %] hx-indicator="#spinner" hx-target="#ob-searchresult_tab_[% current_tab %]" hx-get="[% path_prefix %]/[% config.get('search_loc') %].include?[% facets_query_args_base %];[% default_cgi_querystring %][% IF NOT have_filter.${filterstring} %];f[[% searchprefix %]]=[% uri_escape(searchterm) %][% END %]" href="[% path_prefix %]/[% config.get('search_loc') %].html?[% facets_query_args_base %];[% default_cgi_querystring %][% IF NOT have_filter.${filterstring} %];f[[% searchprefix %]]=[% uri_escape(searchterm) %][% END %]" hx-push-url="[% path_prefix %]/[% config.get('search_loc') %].html?[% facets_query_args_base %];[% default_cgi_querystring %][% IF NOT have_filter.${filterstring} %];f[[% searchprefix %]]=[% uri_escape(searchterm) %][% END %]">[% termitem.0 %] [% IF clndesc %]&nbsp;([% clndesc %])[% END %]</a>  <span class="badge badge-pill badge-secondary ms-auto">[% termitem.1.chunk(-3).join('.') %]</span></li>
[%             END %]
[% IF facets.classification.size > 5 %]
<li class="ob-facet-item ob-more_button"><span class="w-100">[% msg.maketext("Mehr") %] <i class="fa fa-arrow-down"></i></span></li>
<li class="ob-facet-item ob-less_button" hidden><span class="w-100">[% msg.maketext("Weniger") %] <i class="fa fa-arrow-up"></i></span></li>
[% END %]
</ul>
