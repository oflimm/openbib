        <h4 class="ob-facet-title" data-bs-toggle="collapse" data-bs-target="#group-year[% facet_prefix %][% idsuffix %]" aria-expanded="true">
           [% msg.maketext("Erscheinungsjahr") %] 
        </h4>

        <div id="group-year[% facet_prefix %][% idsuffix %]" class="in collapse show">

         <p>
           [% msg.maketext("Von") %]&nbsp;&nbsp;<input id="ob-yearSlider_from[% facet_prefix %][% idsuffix %]" type="text" size="4" title="[% msg.maketext("Eingabe Jahr von") %]" name="year_from" value="[% searchquery.get_searchfield("year_from").val %]" />&nbsp;&nbsp;[% msg.maketext("bis") %]&nbsp;&nbsp;<input id="ob-yearSlider_to[% facet_prefix %][% idsuffix %]" type="text" size="4" title="[% msg.maketext("Eingabe Jahr bis") %]" name="year_to" value="[% searchquery.get_searchfield("year_to").val %]" />&nbsp;&nbsp;<input type="submit" class="btn btn-primary" value="Los"/>
         </p>

        </div>

[% ELSE %]
[%         IF config.get('facets').year && facets.defined('year') && facets.year.size > 0 %]
        <h4 class="ob-facet-title" data-bs-toggle="collapse" data-bs-target="#group-year[% facet_prefix %][% idsuffix %]" aria-expanded="true">
           [% msg.maketext("Erscheinungsjahr") %] 
        </h4>
[%
      minyear = 9999;
      maxyear = -9999;
      
      yearcount = {};
      FOREACH year_thisitem IN facets.year;
        item      = year_thisitem.0;

        itemmatch = item.match('(-?\d\d\d\d)');
	IF itemmatch.0 ;
	  item = itemmatch.0;
	ELSE ;
	   NEXT;
	END;

        itemcount = year_thisitem.1;
        yearcount.${item} = itemcount;
        IF item < minyear ;
	   minyear = item;
	END;
	IF item > maxyear ;
	   maxyear = item;
        END;

      END;

      year_labels = [];
      year_itemcounts = [];
      FOREACH year_label IN yearcount.keys.sort ;
        year_labels.push("'${year_label}'");
        year_itemcounts.push(yearcount.${year_label});
      END;

%]
<div id="group-year[% facet_prefix %][% idsuffix %]" class="in collapse show">
[% IF facets.year.size > 1 %]
<canvas id="ob-yearChart-[% facet_prefix %][% idsuffix %]" width="300" height="200"></canvas>

<div id="ob-yearSlider[% facet_prefix %][% idsuffix %]" class="m-4"></div>

<p>
[% msg.maketext("Von") %]&nbsp;&nbsp;<input id="ob-yearSlider_from[% facet_prefix %][% idsuffix %]" type="text" size="4" name="year_from" title="[% msg.maketext("Eingabe Jahr von") %]" value="[% searchquery.get_searchfield("year_from").val %]" />&nbsp;&nbsp;[% msg.maketext("bis") %]&nbsp;&nbsp;<input id="ob-yearSlider_to[% facet_prefix %][% idsuffix %]" type="text" size="4" name="year_to" title="[% msg.maketext("Eingabe Jahr bis") %]" value="[% searchquery.get_searchfield("year_to").val %]" />&nbsp;&nbsp;<input type="submit" class="btn btn-primary" value="Los"/>
</p>

