<!-- Begin year facet -->
[% IF qopts.get_option('sb') == "eds" || searchprofileid_of_articles == searchquery.get_searchprofile %]
        <h4 class="ob-facet-title" data-bs-toggle="collapse" data-bs-target="#group-year[% facet_prefix %][% idsuffix %]" aria-expanded="true">
           [% msg.maketext("Erscheinungsjahr") %] 
        </h4>

        <div id="group-year[% facet_prefix %][% idsuffix %]" class="in collapse show">

         <p>
           [% msg.maketext("Von") %]&nbsp;&nbsp;<input id="ob-yearSlider_from[% facet_prefix %][% idsuffix %]" type="text" size="4" name="year_from" title="[% msg.maketext("Eingabe Jahr von") %]" value="[% searchquery.get_searchfield("year_from").val %]" />&nbsp;&nbsp;[% msg.maketext("bis") %]&nbsp;&nbsp;<input id="ob-yearSlider_to[% facet_prefix %][% idsuffix %]" type="text" size="4" name="year_to" title="[% msg.maketext("Eingabe Jahr bis") %]" value="[% searchquery.get_searchfield("year_to").val %]" /><button id="ob-year_from_to" class="btn btn-primary ms-3" [% IF offcanvas %]data-bs-toggle="offcanvas"[% END %] hx-indicator="#spinner" hx-target="#ob-searchresult_tab_[% current_tab %]" hx-post="[% path_prefix %]/[% config.get('search_loc') %].include">[% msg.maketext("Los") %]</button>
         </p>

        </div>

[% ELSE %]
[%         IF config.get('facets').year && facets.defined('year') && facets.year.size > 0 %]
        <h4 class="ob-facet-title" data-bs-toggle="collapse" data-bs-target="#group-year[% facet_prefix %][% idsuffix %]" aria-expanded="true">
           [% msg.maketext("Erscheinungsjahr") %] 
        </h4>
[%
      minyear = 9999;
      maxyear = -9999;
      
      yearcount = {};
      FOREACH year_thisitem IN facets.year;
        item      = year_thisitem.0;

        itemmatch = item.match('(-?\d\d\d\d)');
	IF itemmatch.0 ;
	  item = itemmatch.0;
	ELSE ;
	   NEXT;
	END;

        itemcount = year_thisitem.1;
        yearcount.${item} = itemcount;
        IF item < minyear ;
	   minyear = item;
	END;
	IF item > maxyear ;
	   maxyear = item;
        END;

      END;

      year_labels = [];
      year_itemcounts = [];
      FOREACH year_label IN yearcount.keys.sort ;
        year_labels.push("'${year_label}'");
        year_itemcounts.push(yearcount.${year_label});
      END;

%]
<div id="group-year[% facet_prefix %][% idsuffix %]" class="in collapse show">
[% IF facets.year.size > 1 %]
<canvas id="ob-yearChart-[% facet_prefix %][% idsuffix %]" width="300" height="200"></canvas>

<div id="ob-yearSlider[% facet_prefix %][% idsuffix %]" class="m-4"></div>

<p>
[% msg.maketext("Von") %]&nbsp;&nbsp;<input id="ob-yearSlider_from[% facet_prefix %][% idsuffix %]" type="text" size="4" name="year_from" title="[% msg.maketext("Eingabe Jahr von") %]" value="[% searchquery.get_searchfield("year_from").val %]" />&nbsp;&nbsp;[% msg.maketext("bis") %]&nbsp;&nbsp;<input id="ob-yearSlider_to[% facet_prefix %][% idsuffix %]" type="text" size="4" name="year_to" title="[% msg.maketext("Eingabe Jahr bis") %]" value="[% searchquery.get_searchfield("year_to").val %]" /><button id="ob-year_from_to" class="btn btn-primary ms-3" [% IF offcanvas %]data-bs-toggle="offcanvas"[% END %] hx-indicator="#spinner" hx-target="#ob-searchresult_tab_[% current_tab %]" hx-post="[% path_prefix %]/[% config.get('search_loc') %].include">[% msg.maketext("Los") %]</button>
</p>

<script>
var type_ctx[% idsuffix %] = document.getElementById('ob-yearChart-[% facet_prefix %][% idsuffix %]');
var typeChart[% idsuffix %] = new Chart(type_ctx[% idsuffix %], {
    type: 'bar',
    data: {    
        labels: [[% year_labels.join(',') %]],
        datasets: [{
            label: 'Trefferzahl',
            data: [[% year_itemcounts.join(',') %]],
            backgroundColor: 'rgba(54, 162, 235, 0.2)',
            borderColor: 'rgba(54, 162, 235, 1)',
            borderWidth: 1,
            BarThickness: 8,
        }],
    },
    options: {
        legend: {
           display: false
        },
        tooltips: {
          callbacks: {
             label: function(tooltipItem) {
                  return tooltipItem.yLabel;
             }
          }
        },
        scales: {
	   xAxes: [{
                type: 'time',
                time: {
                    unit: 'year'
                }
           }],
           yAxes: [{
                ticks: {
		    stepSize: 1,
                    beginAtZero: true
                }
            }]
        },	
    }
});

[% IF 0 == 1 %]
var yearslider[% idsuffix %] = document.getElementById('ob-yearSlider[% facet_prefix %][% idsuffix %]');

noUiSlider.create(yearslider[% idsuffix %], {
    start: [[% minyear %], [% maxyear %]],
    connect: true,
    range: {
        'min': [% minyear %],
        'max': [% maxyear %]
    }
});

var yearslider_from[% idsuffix %] = document.getElementById('ob-yearSlider_from[% facet_prefix %][% idsuffix %]');
var yearslider_to[% idsuffix %]   = document.getElementById('ob-yearSlider_to[% facet_prefix %][% idsuffix %]');

yearslider[% idsuffix %].noUiSlider.on('update', function (values, handle) {

    var value = values[handle];

    if (handle) {
        yearslider_to[% idsuffix %].value = Math.round(value);
    } else {
        yearslider_from[% idsuffix %].value = Math.round(value);
    }
});

yearslider_from[% idsuffix %].addEventListener('change', function () {
    yearslider[% idsuffix %].noUiSlider.set([this.value, null]);
});

yearslider_to[% idsuffix %].addEventListener('change', function () {
    yearslider[% idsuffix %].noUiSlider.set([null, this.value]);
});
[% END %]

</script>

[% END %] 

        <ul class="list-group">
[%             FOREACH termitem IN facets.year ;
                  LAST IF loop.count > 25 ;
 %]

[%-
    searchterm    = termitem.0;
    searchprefix  = config.get('searchfield').yearstring.prefix;
    filterstring  = "${searchprefix}:${searchterm}";
-%]
<li class="ob-facet-item" [% IF facets.year.size > 5 && loop.count > 5 %]hidden[% END %]>[% IF NOT separate_refine %]<input type="checkbox" name="f[[% searchprefix %]]" value="[% uri_escape(searchterm) %]" [% IF have_filter.${filterstring} %]checked="checked"[% END %] onClick="this.form.submit()"/>&nbsp;[% END %]<a class="w-100" [% IF offcanvas %]data-bs-toggle="offcanvas"[% END %] hx-indicator="#spinner" hx-target="#ob-searchresult_tab_[% current_tab %]" hx-get="[% path_prefix %]/[% config.get('search_loc') %].include?[% facets_query_args_base %];[% default_cgi_querystring %][% IF NOT have_filter.${filterstring} %];f[[% searchprefix %]]=[% uri_escape(searchterm) %][% END %]" href="[% path_prefix %]/[% config.get('search_loc') %].html?[% facets_query_args_base %];[% default_cgi_querystring %][% IF NOT have_filter.${filterstring} %];f[[% searchprefix %]]=[% uri_escape(searchterm) %][% END %]" hx-push-url="[% path_prefix %]/[% config.get('search_loc') %].html?[% facets_query_args_base %];[% default_cgi_querystring %][% IF NOT have_filter.${filterstring} %];f[[% searchprefix %]]=[% uri_escape(searchterm) %][% END %]">[% termitem.0 %]</a>  <span class="badge badge-pill badge-secondary ms-auto">[% termitem.1.chunk(-3).join('.') %]</span></li>
[%             END %]
[% IF facets.year.size > 5 %]
<li class="ob-facet-item ob-more_button"><span class="w-100">[% msg.maketext("Mehr") %] <i class="fa fa-arrow-down"></i></span></li>
<li class="ob-facet-item ob-less_button" hidden><span class="w-100">[% msg.maketext("Weniger") %] <i class="fa fa-arrow-up"></i></span></li>
[% END %]
</ul>
