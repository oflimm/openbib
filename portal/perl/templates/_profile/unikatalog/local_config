     current_profile = searchprofileid_of_web ;
   ELSIF searchprofileid_of_title == searchprofileid_of_view ;
     current_profile = searchprofileid_of_view ;
   ELSE;
     current_profile = searchprofileid_of_books;
   END;

   idsuffix = "";

   current_tab = query.param('tab');
   
