<script type="text/javascript">
$(document).ready(function(){

// Begin Togglen / Einzeltreffer-Historie
$("#history_toggle").html("<b>[<a href=\"#history_anchor\">[% msg.maketext("Mehr") %]</a>]</b>")
// Zuerst verstecken
$("#history").hide();
// und bei Klick Sichtbarkeit togglen
$("#history_toggle").click(function(){
 $("#history").toggle();
 $("#history_toggle").html("");
});
});
</script>

[% recent_titles = session.get_recently_selected_titles %]
[% IF recent_titles.get_size > 0 %]
<div class="ob-session_history"><!-- Begin Historie -->
  <h5 class="ps-3">[% msg.maketext("Diese Titel interessierten Sie bereits") %]</h5>

<!-- <div id="ob-record_title_history_toggle_do"> -->
    <p/>
    <ul class="list-group">
[% FOREACH recent_record IN recent_titles.get_records %]
[%- record_fields = recent_record.get_fields -%]
[%# USE dumper;dumper.dump(record_fields)%]
[%# Personen/Koerperschaften konstruieren %]
[%-

     # Verfasser konstruieren
     persons = [] ;

     personslength = 0 ;
     truncatepers  = 0 ;

     FOREACH part IN record_fields.${'T0100'} ;
       thislength = part.content.length ;
       IF (thislength+personslength) < config.get('maxlength_person') ;
         searchterm    = uri_escape(part.content);
         searchprefix  = config.get('searchfield').personstring.prefix;

         persons.push(part.content);
         personslength = personslength+thislength ;
       ELSE ;
         truncatepers = 1 ;
       END ;
     END ;
     FOREACH part IN record_fields.${'T0101'}  ;
       thiscontent = part.content;
       IF part.supplement ;
          thiscontent = "${thiscontent} ${part.supplement}" ;
       END ;
       thislength = part.content.length ;
       IF (thislength+personslength) < config.get('maxlength_person') ;
         searchterm    = uri_escape(part.content);
         searchprefix  = config.get('searchfield').personstring.prefix;

         persons.push(part.content);
         personslength = personslength+thislength ;
       ELSE ;
         truncatepers = 1 ;
       END ;
     END ;
     FOREACH part IN record_fields.${'T0200'}  ;
       thislength = part.content.length ;
       IF (thislength+personslength) < config.get('maxlength_person') ;
         searchterm    = uri_escape(part.content);
         searchprefix  = config.get('searchfield').corporatebodystring.prefix;

         persons.push(part.content);
         personslength = personslength+thislength ;
       ELSE ;
         truncatepers = 1 ;
       END ;
     END ;
     FOREACH part IN record_fields.${'T0201'}  ;
       thislength = part.content.length ;
       IF (thislength+personslength) < config.get('maxlength_person') ;
         searchterm    = uri_escape(part.content);
         searchprefix  = config.get('searchfield').corporatebodystring.prefix;

         persons.push(part.content);
         personslength = personslength+thislength ;
       ELSE ;
         truncatepers = 1 ;
       END ;
     END ;
     person = persons.join(' ; ') ;
     IF truncatepers ;
       person = "$person [...]" ;
     END ;

     # Titel konstruieren
     title = record_fields.${'T0331'}.first.content;
     IF record_fields.${'T0310'}.first.content;
         title = record_fields.${'T0310'}.first.content;
     END;

     # Mit Unterreihe
     IF record_fields.${'T0360'}.first.content ;
         unterreihe = record_fields.${'T0360'}.first.content;
	 IF NOT title.match(" / ${unterreihe}");
            title = "${title} / ${unterreihe}";
	 END;
     END;

     is_schriftenreihe = 0;
     is_mehrbaendig    = 0;

     IF record_fields.${'T0089'}.first.content;
       is_mehrbaendig = 1;
     ELSIF record_fields.${'T0451'}.first.content;
       is_schriftenreihe = 1;
     END;

     zaehlung = "";
     
     # Zaehlung Anfang Titel
     IF is_mehrbaendig;
       IF record_fields.${'T0090'}.first.content;
          zaehlung = record_fields.${'T0090'}.first.content;
	  IF zaehlung == title ;
	     title = "";
	  END;
          title = "${zaehlung}. ${title}";
       ELSIF record_fields.${'T0089'}.first.content;
          zaehlung = record_fields.${'T0089'}.first.content;
	  IF zaehlung == title ;
	     title = "";
	  END;
          title = "${zaehlung}. ${title}";
       END;     
     END;
     
     # Verlage konstruieren
     publisher = record_fields.${'T0412'}.first.content;

     # Ejahr konstruieren
     yearofpub = record_fields.${'T0425'}.first.content;

     super_title = "";

     # Uebergeordneter Titel in Klammern (super_title)
     IF is_schriftenreihe;
       IF record_fields.${'T0451'}.first.content;
          super_title = record_fields.${'T0451'}.first.content;
       ELSIF super;
         IF super.fields.${'0331'} ;
            super_title   = super.fields.${'0331'}.first.content;
         ELSIF super.fields.${'0451'} ;
            super_title   = super.fields.${'0451'}.first.content;
         END;
       END;
     ELSIF is_mehrbaendig;
       IF super;
         IF super.fields.${'0331'} ;
            super_title   = super.fields.${'0331'}.first.content;
	 END;
       END;
     END;

     IF title && super_title ;
        title = "${title} (${super_title})";
     END;

     # Kein verwertbarer Titel oder Titel in titles_exclude, z.B. bei Ueberordnungen?
     IF NOT title ;
       title = "Keine Titelangabe vorhanden";
     END;

-%]
[% IF recent_titles.get_size > 5 && loop.count == 5 %]
				       <span id="history_toggle"></span>
				       <div id="history">
[% END %]
<li class="list-group-item">[% IF person %][% person %]<br/>[% END %]<a class="inturl" href="[% path_prefix %]/[% config.get('databases_loc') %]/id/[% recent_record.database %]/[% config.get('titles_loc') %]/id/[% recent_record.id %].html"><strong><span class="ob-title">[%
title %]</span></strong></a>, <span class="ob-publisher">[% publisher %]</span> <span class="ob-yearofpub">[% yearofpub %]</span></li>
[% END %]
[% IF recent_titles.get_size > 5 %]
				       </div>
[% END %]
  </ul>
  <p/>

<!-- </div><!-- End ob-record_title_history_toggle_do -->
</div><!-- End Session-Historie -->
[% END %]


