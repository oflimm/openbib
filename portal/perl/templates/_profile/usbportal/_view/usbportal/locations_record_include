    function initLevelSelect() {
        var levelSel = document.getElementById('levelselect');

        levelSel.onchange = function(e) {
            document.getElementById('levelselect').value = this.value;
            searchIndoor();
        }

        var option0 = document.createElement("option");
        option0.value = 0;
        option0.selected = true;
        option0.text = '0';
        levelSel.appendChild(option0);

        var option1 = document.createElement("option");
        option1.value = 1;
        option1.text = '1';
        levelSel.appendChild(option1);


        var indoorSelect = document.getElementById('indoorSelect');
        indoorSelect.onchange = function(e) {
            for (var i = 0; i < feat.length; i++) {
                if (feat[i].properties.name == this.value)
                    document.getElementById('levelselect').value = feat[i].properties.level;
            }
            searchIndoor();
        }
        var highestLevel = 0;
    }


