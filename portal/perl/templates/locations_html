[%#-
#######################################################################
#
# Templatename: locations_include
#
# Typ         : Mastertemplate       
#
# Aufgabe:
#
# Ausgabe einer Uebersichtsliste aller eingerichteten Kataloge
# mit ihren wichtigsten Informationen und einer Standortkarte
#
# Einrichtung eines neuen Kataloges
#
#######################################################################
-%]
[%# USE dumper;dumper.dump(locations)%]

<h3>[% msg.maketext("Standorte der Bibliotheken") %]</h3>

[%-

   is_denied = {
     'DE-Kn3-MAK' = 1
     'DE-Kn3-SL' = 1
     'DE-Kn41' = 1
     'DE-38M' = 1
     'DE-832' = 1
     'DE-887' = 1
     'FMT' = 1
   };

   librariesloc = {};

   FOREACH location IN locations ;
     NEXT IF (is_denied.${location.identifier});
   
     coordinates = location.fields.${"L0280"}.first.content ;
     description = location.description ;
     id          = location.identifier ;

     NEXT IF NOT coordinates;
     
     IF NOT librariesloc.defined(coordinates);
       librariesloc.${coordinates} = [];
     END;

     librariesloc.${coordinates}.push({ description = description, id = id});
   END;

-%]
<p>
<div id="libraries_mapid" style="width: 100%; height: 500px; position:relative;"></div>
</p>

<script type="text/javascript">
var librariesmap = L.map('libraries_mapid', {
    scrollWheelZoom: false,
    dragging: !L.Browser.mobile,
    tap: false
}).setView([50.933777, 6.939545], 13);
L.tileLayer('http://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
        maxZoom: 18,
        attribution: '&copy; <a href="https://openstreetmap.org/copyright">OpenStreetMap contributors</a>'
      }).addTo(librariesmap);


var libraries = [
[% FOREACH coordinates IN librariesloc.keys %]
              [[[% coordinates %]],[ 

[%   FOREACH thisitem IN librariesloc.${coordinates} %]
[ '[% thisitem.description.replace("'",'"') %]','[% thisitem.id %]' ],
[%   END %]
]],
[% END %]
            ];
	    
for (var i = 0; i < libraries.length; i++) {
  var marker = L.marker(libraries[i][0]).addTo(librariesmap);
  var markerstring = "";
  for (var j = 0; j < libraries[i][1].length; j++) {
     markerstring = markerstring + '<a href="[% path_prefix%]/[% config.get('locations_loc') %]/id/'+libraries[i][1][j][1]+'.html?l=[% lang %]">'+libraries[i][1][j][0]+'</a>'
     if (j < libraries[i][1].length -1){
        markerstring = markerstring + ',<br/> ';
     }
  }
  marker.bindPopup(markerstring);
}
</script>

<div class="mt-3">

<p>[% msg.maketext("Hier finden sie eine Liste der Bibliotheken, deren Bestände Sie im Unikatalog durchsuchen können. Neben der USB als zentraler Hochschulbibliothek sind das ca. 150 selbständige Campusbibliotheken, die den Instituten der Universität angeschlossen sind.") %]</p>

</div>

<!-- Erster Tab: Bibliotheken nach Faktulaet -->

<div id="ob-locations_overview">

<ul class="nav nav-tabs" role="tablist">
<li class="nav-item" role="presentation"><a class="nav-link active" role="tab" data-bs-toggle="tab" href="#ob-locations_by_faculty">[% msg.maketext("Bibliotheken nach Fakultät") %]</a></li>
<li class="nav-item" role="presentation"><a class="nav-link" role="tab" data-bs-toggle="tab" href="#ob-locations_a_to_z">[% msg.maketext("Bibliotheken von A-Z") %]</a></li>
[% IF NOT disable_anny %]
<li class="nav-item" role="presentation"><a class="nav-link" role="tab" data-bs-toggle="tab" href="#ob-locations_reservation">[% msg.maketext("Reservierung Arbeitsplätze &amp; Mehr") %]</a></li>
[% END %]
</ul>

<div class="tab-content pt-3 mb-2">

<div role="tabpanel" class="tab-pane fade-in active p-1" id="ob-locations_by_faculty">

<div id="faculties" class="accordion mb-5">
  <div class="card">
    <div class="card-header" id="headingUSB">
      <h5 class="mb-0">
        <a class="card-link collapsed" href="#collapseUSB" data-bs-toggle="collapse" data-bs-target="#collapseUSB" aria-expanded="false" aria-controls="collapseUSB">
          [% msg.maketext("Universitäts- und Stadtbibliothek Köln (USB)") %]
        </a>
      </h5>
    </div>
    <div id="collapseUSB" class="collapse" aria-labelledby="headingUSB" data-parent="#faculties">
      <div class="card-body">

       <ul>
[%  
    institution_map = {};
    FOREACH location IN locations ;
     NEXT IF (is_denied.${location.identifier});
     fakultaet      = location.fields.${"L0015"}.first.content;
     coordinates    = location.fields.${"L0280"}.first.content.split(',').reverse.join(',');

     NEXT IF (fakultaet != "usb");
     NEXT IF (coordinates == "");

     identifier     = location.identifier;

     sigel          = location.fields.${"L0250"}.first.content;
     sigel_path     = location.fields.${"L0250"}.first.content.replace('/','_');

     url = "${path_prefix}/${config.get('locations_loc')}/id/${identifier}";
  
     description    = location.description ;
     arbeitsplaetze = location.fields.${"L0270"}.first.content;
     zeiten         = 'wg. Corona geschlossen' ; # location.fields.${"L0110"}.first.content;

     institution_map.${description} = {
        sigel => sigel,
	sigel_path => sigel_path,
        arbeitsplaetze => arbeitsplaetze,
	url => url,
     };        
   END;
  
%]
[% FOREACH desc IN institution_map.keys.sort %]
	<li>[% IF institution_map.${desc}.url %]<a class="inturl" href="[% scheme %]://[% servername %][% institution_map.${desc}.url %].html?l=[% lang %]" data-bs-toggle="modal" data-bs-target="#genericModal" hx-target="#generic_message" hx-get="[% institution_map.${desc}.url %].include?l=[% lang %]">[% END %][% desc %][% IF institution_map.${desc}.sigel %] ([% institution_map.${desc}.sigel %])[% END %][% IF institution_map.${desc}.url %]</a>[% END %][% IF institution_map.${desc}.arbeitsplaetze %]<br/>Zahl der Nutzerarbeitsplätze: [% institution_map.${desc}.arbeitsplaetze %][% END %]</li>

[% END %]
      </ul>


      </div>
    </div>
  </div><!-- End Card -->

  <div class="card">
    <div class="card-header" id="headingNoFaculty">
      <h5 class="mb-0">
        <a class="card-link collapsed" href="#collapseNoFaculty" data-bs-toggle="collapse" data-bs-target="#collapseNoFaculty" aria-expanded="false" aria-controls="collapseNoFaculty">
          [% msg.maketext("Weitere Fakultätsungebundene / Fakultätsübergreifende Bibliotheken") %]
        </a>
      </h5>
    </div>
    <div id="collapseNoFaculty" class="collapse" aria-labelledby="headingNoFaculty" data-parent="#faculties">
      <div class="card-body">

       <ul>
[%  
