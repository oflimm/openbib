         END ;
         alltags_cloud = "${alltags_cloud}\n</ol>" ;
         clouds_item = {
                    content => alltags_cloud,
                  } ;
         normdata.${"T4500"} = [ clouds_item ];
      END ;
  END ;
%]
[%  IF config.get('social_tagging') && all_tags.size > 0 %]
