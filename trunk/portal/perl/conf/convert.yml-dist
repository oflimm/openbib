# Parametrisierung der Sortierung
    sorting_order:
      - 'P0800'
      - 'C0800'
      - 'S0800'
      - 'PC0001'
      - 'T0331'
      - 'T5100'
      - 'T0425' 
      - 'T0424' # 424 hiner 425, d.h. 424 wichtiger, falls besetzt und gleiches Sortierungsfeld
      - 'T0412'
      - 'X0014'
      - 'popularity'
    sorting:
      'P0800':
        'sortfield': 'authority' # Normdaten-Ansetzungsform Person
        'type': 'stringfield'
      'C0800':
        'sortfield': 'authority' # Normdaten-Ansetzungsform Koerperschaft
        'type': 'stringfield'
      'S0800':
        'sortfield': 'authority' # Normdaten-Ansetzungsform Schlagwort
        'type': 'stringfield'
      'PC0001':
        'sortfield': 'person'    # Verfasser/Koepeschaft
        'type': 'stringfield'
      'T0331':                   # Titel
        'sortfield': 'title'
        'type': 'stringfield'
        'filter':
           - 'filter_strip_nonsortable_word'
      'T5100':
        'sortfield': 'order'     # Zaehlung
        'type': 'integerfield'
      'T0425':
        'sortfield': 'year'      # Jahr
        'type': 'integerfield'       
      'T0424':
        'sortfield': 'year'      # Jahr
        'type': 'integerfield'
        'filter':
           - 'filter_force_signed_year'
      'T0412':
        'sortfield': 'publisher' # Verlag
        'type': 'stringfield'
      'X0014':
        'sortfield': 'mark'      # Signatur
        'type': 'stringfield'
      'popularity':
        'sortfield': 'popularity'# Popularitaet
        'type': 'integervalue'

