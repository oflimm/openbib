# Versionsnummer
version: OpenBib 3 / alpha5

# Unter dieser IP wird der lokale Server angesprochen
local_ip: 127.0.0.1

# Einstellungen fuer den Zugriff auf die Katalog-Datenbanken
dbimodule: Pg
pgdbhost: localhost
pgdbport: 5432
pgdbname: postgres
pgdbuser: root
pgdbpasswd: StrengGeheim

# Einstellungen fuer den Zugriff auf die Katalog-Datenbanken
dbimodule: Pg
dbhost: localhost
dbport: 5432
dbuser: root
dbpasswd: StrengGeheim

# Einstellungen fuer den Zugriff auf die System-Datenbank
systemdbimodule: Pg
systemdbhost: localhost
systemdbport: 5432
systemdbname: openbib_system
systemdbuser: root
systemdbpasswd: StrengGeheim

# Einstellungen fuer den Zugriff auf die Enrichmnt-Datenbank
enrichmntdbimodule: Pg
enrichmntdbhost: localhost
enrichmntdbport: 5432
enrichmntdbname: openbib_enrichmnt
enrichmntdbuser: root
enrichmntdbpasswd: StrengGeheim

# Einstellungen fuer den Zugriff auf die Statistik-Datenbank
statisticsdbimodule: Pg
statisticsdbhost: localhost
statisticsdbport: 5432
statisticsdbname: openbib_statistics
statisticsdbuser: root
statisticsdbpasswd: StrengGeheim

# Einstellungen fuer Memcached

#memcached:
  #servers:
     #- '127.0.0.1:11211'
  #compress_threshold: 10000

# Einstellungen fuer ElasticSearch

elasticsearch:
  servers: '127.0.0.1:9200'
  transport: 'httplite'
  max_requests: 10000
#  trace_calls: '/var/log/openbib/elasticsearch.log'
  no_refresh: 1

local_search_backend: xapian

# Benutzer, Passwort und Default view fuer die Administration
adminuser: admin
adminpasswd: StrengGeheim

defaultview: openbib

# RSS-Typen
rss_types:
  neuerwerbung: 99
  neuzugang: 1
  neuzugang_nach_aut: 2
  neuzugang_nach_kor: 3
  neuzugang_nach_swt: 4
  neuzugang_nach_sys: 5

location_types:
  - ISIL
  - Generic

source_systems:
  - Sisis
  - Aleph
  - Allegro
  - Lars
  - OAI
  - MAB
  - 'Backend: BibSonomy'
  - 'Backend: Z3950'
  - 'Backend: EZB'
  - 'Backend: DBIS'
  - Anderes

# Spracheinstellungen
lang:
  - de
  - en

# Basis-Pfad
base_loc: /portal

# Mapping der einzelnen Kompontenten zu ihren Zugriffs-URI's

## Systmweite Resourcen, Aenderbar fuer Web-Administration
admin_loc: admin
authenticators_loc: authenticators
databases_loc: databases
views_loc: views
locations_loc: locations
profiles_loc: profiles
orgunits_loc: orgunits
sessions_loc: sessions
searchprofiles_loc: searchprofiles
users_loc: users
roles_loc: roles
rss_loc: rss
statistics_loc: statistics
servers_loc: servers
clusters_loc: clusters
topics_loc: topics

## Konnektoren
connector_availabilityimage_loc: connector/availabilityimage
connector_availability_loc: connector/availability
connector_bibmanager_loc: connector/literaturverwaltung
connector_digibib_loc: connector/digibib
connector_livesearch_loc: connector/livesearch
connector_locationmark_loc: connector/sykasignatur
connector_olws_loc: connector/olws
# Legacy URI - Ab v2.4 kann jeder URL direkt gebookmarkt werden
connector_permalink_loc: connector/permalink
connector_qrcode_loc: connector/qrcode
connector_rss_loc: connector/rss
connector_seealso_loc: connector/seealso
connector_similarpersons_loc: connector/similarpersons
connector_similarsubjects_loc: connector/similarsubjects
connector_spellcheck_loc: connector/spellcheck
connector_ukonline_loc: connector/uk-online
connector_unapi_loc: connector/unapi

## Resourcen mit dedizierten IDs
classifications_loc: classifications
clouds_loc: clouds
corporatebodies_loc: corporatebodies
databases_loc: databases
locations_loc: locations
litlists_loc: litlists
litlists_public_loc: litlists
litlists_public_topics_loc: litlists/topics
litlists_public_users_loc: litlists/users
persons_loc: persons
reviews_loc: reviews
subjects_loc: subjects
tags_loc: tags
titles_loc: titles
topics_loc: topics
users_loc: users

## Allgemeine URIs
#tags_loc: tags

## Funktionelle URIs 
browse_loc: browse
circulations_loc: circulations
preferences_loc: preferences
collection_loc: collection
cartitems_loc: cartitems
home_loc: home
index_loc: index
info_loc: info
login_loc: login
logout_loc: logout
mailcollection_loc: mailcollection
passwords_loc: passwords
redirect_loc: redirect
registrations_loc: registrations
search_history_loc: search/history
search_loc: search
searchforms_loc: searchforms
searchprofiles_loc: searchprofiles
serveractive_loc: server_is_active
#userreviews_loc: review

dispatch_rules:

  - # review_loc:
     module: 'OpenBib::Handler::Apache::Reviews'
     rule: '/portal/:view/reviews/isbn/:isbn[get]'
     runmode: 'show_collection_by_isbn'

  - # index_loc:
     module: 'OpenBib::Handler::Apache::Search'
     rule: '/portal/:view/index/:type/*'
     runmode: 'index'

# Charakteristik
# - ausschliesslich oeffentliche Repraesentation [get]
# - ein kanonischer Resource-URI [get]
# - Verlinkung auf nutzerspezifische Repreaesentation anderer
#   Resourcen ueber deren Resource-URL, der entsprechend weiterleitet

#####################################################################
# Recherche-Resource/Repraesentation

  - # Resource: search_loc
     module: 'OpenBib::Handler::Apache::Search'
     rule: '/portal/:view/search[get]'
     representations:
       - none
       - html
       - json
       - rdf
       - rss
       - mobile
     runmode: 'show_search'
  - # Resource: search_loc
     module: 'OpenBib::Handler::Apache::Search::History'
     rule: '/portal/:view/search/history[get]'
     representations:
       - none
       - html
     runmode: 'show'

#####################################################################
# Browse-Resource/Repraesentation

  - # Resource: browse_loc
     module: 'OpenBib::Handler::Apache::Browse'
     rule: '/portal/:view/browse[get]'
     representations:
       - none
       - html
       - mobile
     runmode: 'show_collection'
  - # Resource: browse_loc
     module: 'OpenBib::Handler::Apache::Browse::Topics'
     rule: '/portal/:view/browse/topics[get]'
     representations:
       - none
       - html
     runmode: 'show_collection'
  - # Resource: browse_loc
     module: 'OpenBib::Handler::Apache::Browse::Topics'
     rule: '/portal/:view/browse/topics/id/:topicid[get]'
     representations:
       - none
       - html
     runmode: 'show_record'
  - # Resource: browse_loc
     module: 'OpenBib::Handler::Apache::Browse::BK'
     rule: '/portal/:view/browse/bks[get]'
     representations:
       - none
       - html
     runmode: 'show_collection'
  - # Resource: browse_loc
     module: 'OpenBib::Handler::Apache::Browse::BK'
     rule: '/portal/:view/browse/bks/id/:bkid[get]'
     representations:
       - none
       - html
     runmode: 'show_record'
  - # Resource: browse_loc
     module: 'OpenBib::Handler::Apache::Browse::BK'
     rule: '/portal/:view/browse/bks/id/:tlbkid/id/:bkid[get]'
     runmode: 'show_record_databases'

#####################################################################
# Informations-Resource/Repraesentation

  - # info_loc:
     module: 'OpenBib::Handler::Apache::Info'
     rule: '/portal/:view/info/:stid'
     runmode: 'show_record'

#####################################################################
# Redirect-Resource/Repraesentation

  - # redirect_loc:
     module: 'OpenBib::Handler::Apache::Redirect'
     rule: '/portal/:view/redirect'
     runmode: 'show'
     args:
        disable_content_negotiation: 1

#####################################################################
# RSS-Feed Informations-Resource/Repraesentation

  - # rssfeeds_loc:
     module: 'OpenBib::Handler::Apache::RSSFeeds'
     rule: '/portal/:view/rss/view[get]'
     representations:
