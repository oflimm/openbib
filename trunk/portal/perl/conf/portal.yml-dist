  - # Resource: users_loc/tags_loc: Create Record
     module: 'OpenBib::Handler::Apache::Users::Tags'
     rule: '/portal/:view/tags[post]'
     runmode: 'create_record'
  - # Resource: tags_loc: Delete Record
     module: 'OpenBib::Handler::Apache::Users::Tags'
     rule: '/portal/:view/tags/id/:tagid[delete]'
     runmode: 'delete_record'
  - # Resource: tags_loc: Update Record
     module: 'OpenBib::Handler::Apache::Users::Tags::Names'
     rule: '/portal/:view/tags/names/id/:tagname[post]'
     runmode: 'update_record'
  - # Resource: tags_loc: Update Record
     module: 'OpenBib::Handler::Apache::Users::Tags::Names'
     rule: '/portal/:view/tags/names/id/:tagname[put]'
     runmode: 'update_record'

# Anzeige privater Listen ueber separaten user_loc

