# Charakteristik
# - oeffentliche und user Repraesentation [get]
# - ein kanonischer Resource-URI [get]
# - Automatische Weiterleitung von [get] auf ggf. nutzerspezifische Repreaesentation

#####################################################################
# Startseiten-Resource/Repraesentation
  - # Resource: home_loc
     module: 'OpenBib::Handler::Apache::Home'
     rule: '/portal/:view/home[get]'
     representations:
       - none
       - html
       - mobile
     args:
        users_loc: 'home'        
     runmode: 'show'
  - # Resource: users_loc/home_loc
     module: 'OpenBib::Handler::Apache::Home'
     rule: '/portal/:view/users/id/:userid/home[get]'
     representations:
       - none
       - html
       - mobile
     runmode: 'show'

# Charakteristik
# - verschiedene oeffentliche und user Repraesentation [get]
# - ein kanonischer Resource-URI [post,put,delete]
# - *keine* automatische Weiterleitung

#####################################################################
# Literaturlisten-Resource/Repraesentation

  - # Resource: litlists_loc: Alle oeffentliche Literaturlisten
     module: 'OpenBib::Handler::Apache::LitLists'
     rule: '/portal/:view/litlists[get]'
     representations:
       - none
       - html
       - json
       - include
       - rdf
     runmode: 'show_collection'

  - # Resource: litlists_loc: Alle zuletzt angelegten oeffentliche Literaturlisten
     module: 'OpenBib::Handler::Apache::LitLists'
     rule: '/portal/:view/litlists/recent[get]'
     representations:
       - none
       - html
       - json
       - rss
       - include
       - rdf
     runmode: 'show_collection_recent'

  - # Resource: litlists_loc: Themengebiete, nach denen alle oeffentlichen Literaturlisten
    # kategorisiert sind
     module: 'OpenBib::Handler::Apache::LitLists'
     rule: '/portal/:view/litlists/topics[get]'
     representations:
       - none
       - html
       - json
       - rdf
     runmode: 'show_collection_by_topic'

  - # Resource: litlists_loc: oeffentlichen Literaturlisten zu einem Topic
    # kategorisiert sind
     module: 'OpenBib::Handler::Apache::LitLists'
     rule: '/portal/:view/litlists/topics/id/:topicid[get]'
     runmode: 'show_collection_by_single_topic'

  - # Resource: litlists_loc: zu letzt angelegte oeffentliche Literaturlisten zu einem Topic
     module: 'OpenBib::Handler::Apache::LitLists'
     rule: '/portal/:view/litlists/topics/id/:topicid/recent[get]'
     representations:
       - none
       - html
       - json
       - rss
       - include
       - rdf
     runmode: 'show_collection_by_single_topic_recent'

  - # Resource: litlists_loc: oeffentlichen Literaturlisten von einem User
     module: 'OpenBib::Handler::Apache::LitLists'
     rule: '/portal/:view/litlists/users/id/:userid[get]'
     runmode: 'show_collection_by_single_user'

  - # Resource: users_loc/litlists_loc: Anzeige einer Literaturliste (oeffentlich/privat)
     module: 'OpenBib::Handler::Apache::LitLists'
     rule: '/portal/:view/litlists/id/:litlistid[get]'
     runmode: 'show_record'
     args:
        users_loc: 'litlists'        

  - # Resource: users_loc/litlists_loc: Anzeige einer Literaturliste (oeffentlich/privat)
     module: 'OpenBib::Handler::Apache::Users::LitLists'
     rule: '/portal/:view/users/id/:userid/litlists/id/:litlistid[get]'
     runmode: 'show_record'

  - # Resource: users_loc/litlists_loc: Create Record
     module: 'OpenBib::Handler::Apache::Users::LitLists'
     rule: '/portal/:view/litlists[post]'
     runmode: 'create_record'
  - # Resource: litlists_loc: Update Record
     module: 'OpenBib::Handler::Apache::Users::LitLists'
     rule: '/portal/:view/litlists/id/:litlistid[post]'
     runmode: 'update_record'
  - # Resource: litlists_loc: Update Record
     module: 'OpenBib::Handler::Apache::Users::LitLists'
     rule: '/portal/:view/litlists/id/:litlistid[put]'
     runmode: 'update_record'
  - # Resource: litlists_loc: Delete Record
     module: 'OpenBib::Handler::Apache::Users::LitLists'
     rule: '/portal/:view/litlists/id/:litlistid[delete]'
     runmode: 'delete_record'

  - # Resource: users_loc/litlists_loc: Show Items ?entfernen?
     module: 'OpenBib::Handler::Apache::Users::LitLists::Item'
     rule: '/portal/:view/litlists/id/:litlistid/items[get]'
     representations:
       - none
       - html
       - json
     runmode: 'show_collection'

  - # Resource: users_loc/litlists_loc: Show Single Item ?entfernen?
     module: 'OpenBib::Handler::Apache::Users::LitLists::Item'
     rule: '/portal/:view/litlists/id/:litlistid/items/id/:itemid[get]'
     runmode: 'show_record'

  - # Resource: users_loc/litlists_loc: Create Item
     module: 'OpenBib::Handler::Apache::Users::LitLists::Item'
     rule: '/portal/:view/litlists/id/:litlistid/items[post]'
     runmode: 'create_record'
  - # Resource: litlists_loc: Update Item
     module: 'OpenBib::Handler::Apache::Users::LitLists::Item'
     rule: '/portal/:view/litlists/id/:litlistid/items/id/:itemid[post]'
     runmode: 'update_record'
  - # Resource: litlists_loc: Update Item
     module: 'OpenBib::Handler::Apache::Users::LitLists::Item'
     rule: '/portal/:view/litlists/id/:litlistid/items/id/:itemid[put]'
     runmode: 'update_record'
  - # Resource: litlists_loc: Delete Item
     module: 'OpenBib::Handler::Apache::Users::LitLists::Item'
     rule: '/portal/:view/litlists/id/:litlistid/items/id/:itemid[delete]'
     runmode: 'delete_record'

# Anzeige privater Listen ueber separaten user_loc

  - # Resource: users_loc/litlists_loc: Anzeige aller eigenen Literaturlisten
     module: 'OpenBib::Handler::Apache::Users::LitLists'
     rule: '/portal/:view/users/id/:userid/litlists[get]'
     representations:
       - none
       - html
       - json
       - rdf
     runmode: 'show_collection'

  - # Resource: users_loc/litlists_loc: Edit einer eigenen Literaturliste
     module: 'OpenBib::Handler::Apache::Users::LitLists'
     rule: '/portal/:view/users/id/:userid/litlists/id/:litlistid/edit[get]'
     representations:
       - none
       - html
     runmode: 'show_record_form'


#####################################################################
# Tags-Resource/Repraesentation

  - # Resource: tags_loc: Alle oeffentliche Tags
     module: 'OpenBib::Handler::Apache::Tags'
     rule: '/portal/:view/tags[get]'
     representations:
       - none
       - html
