# Versionsnummer
version: OpenBib v2.4alpha3

# Unter dieser IP wird der lokale Server angesprochen
local_ip: 127.0.0.1

# Einstellungen fuer den Zugriff auf die Katalog-Datenbanken
dbimodule: Pg
pgdbhost: localhost
pgdbport: 5432
pgdbname: postgres
pgdbuser: root
pgdbpasswd: StrengGeheim

# Einstellungen fuer den Zugriff auf die Katalog-Datenbanken
dbimodule: Pg
dbhost: localhost
dbport: 5432
dbuser: root
dbpasswd: StrengGeheim

# Einstellungen fuer den Zugriff auf die System-Datenbank
systemdbimodule: Pg
systemdbhost: localhost
systemdbport: 5432
systemdbname: openbib_system
systemdbuser: root
systemdbpasswd: StrengGeheim

# Einstellungen fuer den Zugriff auf die Enrichmnt-Datenbank
enrichmntdbimodule: Pg
enrichmntdbhost: localhost
enrichmntdbport: 5432
enrichmntdbname: openbib_enrichmnt
enrichmntdbuser: root
enrichmntdbpasswd: StrengGeheim

# Einstellungen fuer den Zugriff auf die Statistik-Datenbank
statisticsdbimodule: Pg
statisticsdbhost: localhost
statisticsdbport: 5432
statisticsdbname: openbib_statistics
statisticsdbuser: root
statisticsdbpasswd: StrengGeheim

# Einstellungen fuer Memcached

#memcached:
  #servers:
     #- '127.0.0.1:11211'
  #compress_threshold: 10000

# Einstellungen fuer ElasticSearch

elasticsearch:
  servers: '127.0.0.1:9200'
  transport: 'httplite'
  max_requests: 10000
#  trace_calls: '/var/log/openbib/elasticsearch.log'
  no_refresh: 1

local_search_backend: xapian

# Benutzer, Passwort und Default view fuer die Administration
adminuser: admin
adminpasswd: StrengGeheim

defaultview: openbib

# RSS-Typen
rss_types:
  neuerwerbung: 99
  neuzugang: 1
  neuzugang_nach_aut: 2
  neuzugang_nach_kor: 3
  neuzugang_nach_swt: 4
  neuzugang_nach_sys: 5

source_systems:
  - Sisis
  - Aleph
  - Allegro
  - Lars
  - OAI
  - MAB
  - 'Backend: BibSonomy'
  - 'Backend: Z3950'
  - 'Backend: EZB'
  - 'Backend: DBIS'
  - Anderes

# Spracheinstellungen
lang:
  - de
  - en

# Basis-Pfad
base_loc: /portal

# Mapping der einzelnen Kompontenten zu ihren Zugriffs-URI's
