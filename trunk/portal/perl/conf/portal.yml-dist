base_dir: /opt/openbib
conv_dir: /opt/openbib/conv
dbdesc_dir: /opt/openbib/db
tool_dir: /opt/openbib/bin

# Zugriffsinformationen fuer das BibSonomy-API
bibsonomy_api_key: ''
bibsonomy_api_user: ''

# Zugriffsinformationen fuer die LibraryThing-APIs
