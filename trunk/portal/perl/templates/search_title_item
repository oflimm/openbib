[%#-
#######################################################################
#
# Templatename: admin_database
#
# Typ         : Mastertemplate       
#
# Aufgabe:
#
# Ausgabe einer Uebersichtsliste aller eingerichteten Kataloge
# mit ihren wichtigsten Informationen
#
# Einrichtung eines neuen Kataloges
#
#######################################################################
-%]
[%- PROCESS common/subtemplate name="local_config" -%] 
[%- IF representation == '' || representation == 'html' -%]
[%- PROCESS common/subtemplate name="search_titles_item_html" -%]
[%- ELSIF representation == 'rdf' -%]
[%- PROCESS common/subtemplate name="search_titles_item_rdf" -%]
[%- ELSIF representation == 'json' -%]
[%- PROCESS common/subtemplate name="search_titles_item_json" -%]
[%- ELSIF representation == 'rss' -%]
[%- PROCESS common/subtemplate name="search_titles_item_rss" -%]
[%- ELSIF representation == 'include' -%]
[%- PROCESS common/subtemplate name="search_titles_item_include" -%]
[%- ELSE -%]
This Resource-Type is not available in the representation [% representation %]
[%- END -%]
