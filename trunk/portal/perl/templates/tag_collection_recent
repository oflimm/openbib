[%- IF representation == '' || representation == 'html' -%]
[%- PROCESS common/subtemplate name="tag_collection_recent_html" -%]
[%- ELSIF representation == 'rdf' -%]
[%- PROCESS common/subtemplate name="tag_collection_recent_rdf" -%]
[%- ELSIF representation == 'json' -%]
[%- PROCESS common/subtemplate name="tag_collection_recent_json" -%]
[%- ELSIF representation == 'rss' -%]
[%- PROCESS common/subtemplate name="tag_collection_recent_rss" -%]
[%- ELSIF representation == 'include' -%]
[%- PROCESS common/subtemplate name="tag_collection_recent_include" -%]
[%- ELSE -%]
This Resource-Type is not available in the representation [% representation %]
[%- END -%]
