[%- PROCESS common/subtemplate name="title_rdf" -%]
[%- ELSIF representation == 'json' -%]
[%- PROCESS common/subtemplate name="title_json" -%]
[%- ELSIF representation == 'include' -%]
[%- PROCESS common/subtemplate name="title_include" -%]
[%- ELSE -%]
This Resource-Type is not available in the representation [% representation %]
[%- END -%]
